<% flash.each do |type, message| %>
  <div class="alert alert-<%= type %>">
    <%= message %>
  </div>
<% end %>


<%= form_tag  create_withdrawal_fiat_path(@user), method: :post do %>
  <%= label_tag :amount, "Amount to withdraw" %>
  <%= text_field_tag :amount %>
  <%= submit_tag "Withdraw" %>
<% end %>
