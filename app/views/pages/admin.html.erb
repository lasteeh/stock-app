<p>Admin Dashboard</p>
<% if @users.nil? %>
  <p>No users available</p>
<% else %>
  <table>
    <thead>
      <tr>
        <th>Email</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <% unless user.isadmin %>
          <tr>
            <td class="border px-4 py-2"><%= user.email %></td>
            <td class="border px-4 py-2"><%= user.first_name %></td>
            <td class="border px-4 py-2"><%= user.last_name %></td>
            <td class="border px-4 py-2"><%= user.status %></td>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
<% end %>
